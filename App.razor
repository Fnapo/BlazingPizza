<Router AppAssembly="typeof(Program).Assembly" Context="routeData">
    <Found>
        <RouteView RouteData="routeData" DefaultLayout="typeof(PlanPrincipal)" />
    </Found>
    <NotFound>
        <LayoutView Layout="typeof(PlanPrincipal)">
            <div class="main h-85vh flex-centrado">Sorry, there's nothing at this address.</div>
        </LayoutView>
    </NotFound>
</Router>
